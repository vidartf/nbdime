{
  "name": "nbdime-jupyterlab",
  "version": "0.1.0",
  "description": "A JupyterLab extension for showing Notebook diffs.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "dependencies": {
    "@jupyterlab/application": "^0.16.0",
    "@jupyterlab/apputils": "^0.16.0",
    "@jupyterlab/docregistry": "^0.16.0",
    "@jupyterlab/notebook": "^0.16.0",
    "@jupyterlab/rendermime": "^0.16.0",
    "@jupyterlab/services": "^2.0.0",
    "@phosphor/coreutils": "^1.3.0",
    "@phosphor/disposable": "^1.1.2",
    "@phosphor/widgets": "^1.5.0",
    "nbdime": "^0.4.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.2",
    "typescript": "~2.8.1"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "notebook"
  ],
  "jupyterlab": {
    "extension": true
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "tsc --project src",
    "update": "rimraf node_modules/nbdime && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyter/nbdime"
  },
  "files": [
    "lib/*.js",
    "style/*.css"
  ],
  "author": "Project Jupyter",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jupyter/nbdime/issues"
  },
  "homepage": "https://github.com/jupyter/nbdime"
}
